    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "browser-sync": "^2.14.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
